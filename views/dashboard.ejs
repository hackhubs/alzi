          <li><i class="fa fa-envelope fa-2x"></i> test@acme.test</li>
        </ul>
      </div>
      <div class="contact">
        <marquee color="black" width="100%" direction="right" scrollamount="12" height="60px">
          <h1>Welcome to our Services.</h1>			
          </marquee>
        <div class="alert">Your message has been sent</div>
        <form id="contactForm">
          <p>
            <label>Name</label>
            <input type="text" name="name" id="name" required>
          </p>
          <p>
            <label>No. of Patients</label>
            <input type="text" name="company" id="company">
          </p>
          <p>
            <label>Email Address</label>
            <input type="email" name="email" id="email" required>
          </p>
          <p>
            <label>Phone Number</label>
            <input type="text" name="phone" id="phone">
          </p>
          
  <label for="dob">Date of Birth:</label>
  <div class="empty-width"></div>
  <input class="medium-sized-input-box" placeholder="Date of Birth" type="date" class="form-control" name="dob" id="dob" />
  
  <div class="empty-width"></div>
  <label for="dob">Date of Appointment:</label>
  <div class="empty-width"></div>
  <input class="medium-sized-input-box" placeholder="Date of Birth" type="date" class="form-control" name="dob" id="dob" />
  <div class="empty-width"></div>

  <label for="city">City:</label>
  <input class="medium-sized-input-box" placeholder="City" type="text" class="form-control" name="city" id="city" />

  <label for="state">State:</label>
  <input class="medium-sized-input-box" placeholder="State" type="text" class="form-control" name="state" id="state" />

<label for="country">Country</label><select name="country" id="country" medium-sized-input-box>
<option value="India"selected>India</option>
<option value="USA">USA</option>
<option value="UAE">UAE</option>
</select> 


<label class="address-label"for="address">Address</label><textarea rows="5" cols="50" name="Address" id="address"></textarea>     
